{
  "name": "checkpoint-redis",
  "version": "0.1.0",
  "description": "langchain redis checkpoint saver",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "test": "glob -c \"tsx --test \" \"src/**/*.test.ts\"",
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc",
    "prepack": "npm run build",
    "prepare": "npm run build",
    "typecheck": "tsc --noEmit",
    "lint": "npm run lint:eslint && npm run lint:dpdm",
    "lint:eslint": "NODE_OPTIONS=--max-old-space-size=4096 eslint --cache --ext .ts,.js src/",
    "lint:dpdm": "dpdm --exit-code circular:1 --no-warning --no-tree src/*.ts src/**/*.ts",
    "format": "prettier --config .prettierrc --write \"src\"",
    "format:check": "prettier --config .prettierrc --check \"src\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/levivoelz/checkpoint-redis.git"
  },
  "keywords": [
    "langchain",
    "langgraph",
    "redis",
    "checkpoint",
    "redis-saver"
  ],
  "author": "Levi Voelz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/levivoelz/checkpoint-redis/issues"
  },
  "homepage": "https://github.com/levivoelz/checkpoint-redis#readme",
  "peerDependencies": {
    "@langchain/core": "^0.3.78",
    "@langchain/langgraph-checkpoint": "^0.1.1",
    "ioredis": "^5.8.1"
  },
  "devDependencies": {
    "@langchain/langgraph-checkpoint": "^0.1.1",
    "@tsconfig/recommended": "^1.0.3",
    "@types/ioredis-mock": "^8.2.5",
    "@types/node": "^24.7.2",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "dpdm": "^3.12.0",
    "eslint": "^9.37.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-no-instanceof": "^1.0.1",
    "eslint-plugin-prettier": "^5.5.4",
    "glob": "^11.0.0",
    "ioredis-mock": "^8.13.0",
    "prettier": "^3.6.2",
    "tsx": "^4.17.0",
    "typescript": "^5.9.3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src"
  ]
}
